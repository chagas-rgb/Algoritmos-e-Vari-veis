def calcular_soma_pares(inicio, fim):
    soma = 0
    for num in range(inicio, fim + 1):
        if num % 2 == 0:
            soma += num
    else:
        if soma == 0:
            print("Não há números pares no intervalo fornecido.")
        else:
            print("A soma dos números pares no intervalo é:", soma)

def main():
    # Solicitar entrada do usuário para o início e o fim do intervalo
    inicio = int(input("Digite o início do intervalo: "))
    fim = int(input("Digite o fim do intervalo: "))

    # Verificar e calcular a soma dos números pares no intervalo
    calcular_soma_pares(inicio, fim)

if __name__ == "__main__":
    main()
